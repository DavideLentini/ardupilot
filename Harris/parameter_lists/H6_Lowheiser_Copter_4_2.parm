#INS_HNTC2_BW 22
#INS_HNTC2_ENABLE 1
#INS_HNTC2_FREQ 37
#INS_HNTC2_HMNCS 15
#INS_HNTC2_MODE 1
#INS_HNTC2_OPTS 1
