#INS_HNTCH_BW 15.5
#INS_HNTCH_ENABLE 1
#INS_HNTCH_FREQ 31
#INS_HNTCH_HMNCS 15
#INS_HNTCH_MODE 1
#INS_HNTCH_OPTS 1
#INS_HNTCH_REF 0.25

